       http://www.springframework.org/schema/beans/spring-beans-4.3.xsd
">

    <bean id="osramEngine" class="org.example.impls.OSRAM.EngineImpl" scope="prototype"/>
    <bean id="osramBody" class="org.example.impls.OSRAM.BodyImpl" scope="prototype"/>
